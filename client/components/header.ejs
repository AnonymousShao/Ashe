<!DOCTYPE html>
<head>
  <title><%= title %></title>
  <meta name="viewport" content="initial-scale=1.0, user-scalable=no, maximum-scale=1, width=device-width">
  <link rel="stylesheet" type="text/css" href="/static/common/style/index.css" />
  <link rel="stylesheet" type="text/css" href="<%=cssFile %>" />
  <link href="/static/assets/favicon.ico" mce_href="/static/assets/favicon.ico" rel="icon" type="image/x-icon" /> 
</head>
<body >
<div id="volcano-global-header" class="volcano-global-header">
  <div class="header-wrapper">
    <div class="header-left">
      <a href="/"></a>
      <div id="x-open" class="x-wrapper">
        <div class="x-1"></div>
        <div class="x-2"></div>
      </div>
    </div>
    <ul id="header-center" class="header-center">
      <li>
        <a href="javascript:void(0)" class="uos-product">
          <%= produce %>
          <i class="icon-arrow-down"></i>
        </a>
        <div class="dropdown-wrapper">
          <ul class="dropdown">
            <li><a href="/product/uos-enterprise">UOS Enterprise</a></li>
            <li><a href="/product/uos-director">UOS Director</a></li>
            <li><a href="/product/uos-halo">UOS Manager</a></li>
            <li><a href="/product/uos-operation-monitor">UOS Operator</a></li>
          </ul>
        </div>
      </li>
      <li>
        <a href="javascript:void(0)">
          <%= stories %>
          <i class="icon-arrow-down"></i>
        </a>
        <div class="dropdown-wrapper">
          <ul class="dropdown">
            <li><a href="/solution/clients"><%= clients %></a></li>
            <li><a href="/solution/partners"><%= partners %></a></li>
          </ul>
        </div>
      </li>
      <li class="multi-dropdown">
        <a href="javascript:void(0)">
          <%= services_support %>
          <i class="icon-arrow-down"></i>
        </a>
        <div class="dropdown-wrapper">
          <ul class="dropdown">
            <li class="multi select">
              <a href="javascript:void(0)">
                <%= private_cloud %>
                <i class="icon-arrow-right"></i>
                <i class="icon-arrow-down"></i>
              </a>
              <ul class="child-dropdown">
                <li><a href="/private-cloud/private-service"><%= services %></a></li>
                <li><a href="/private-cloud/private-skill"><%= support %></a></li>
                <li><a href="/private-cloud/private-start"><%= online_process %></a></li>
                <li><a href="/private-cloud/private-apply"><%= private_apply %></a></li>
              </ul>
            </li>
            <li class="multi">
              <a href="javascript:void(0)">
                <%= support %>
                <i class="icon-arrow-right"></i>
                <i class="icon-arrow-down"></i>
              </a>
              <ul class="child-dropdown">
                <li><a href="/blog?page=1"><%= blog %></a></li>
                <li><a href="/docs"><%= uos3 %></a></li>
              </ul>
            </li>
            <li class="multi">
              <a href="javascript:void(0)">
                <%= train %>
                <i class="icon-arrow-right"></i>
                <i class="icon-arrow-down"></i>
              </a>
              <ul class="child-dropdown">
                <li><a href="/training/train"><%= train %></a></li>
                <li><a href="/training/train-list"><%= train_list %></a></li>
                <li><a href="/training/train-apply"><%= train_apply %></a></li>
              </ul>
            </li>
          </ul>
        </div>
      </li>
      <li>
        <a href="javascript:void(0)">
          <%= about_us_header %>
          <i class="icon-arrow-down"></i>
        </a>
        <div class="dropdown-wrapper">
          <ul class="dropdown">
            <li><a href="/about/milestones"><%= milestones %></a></li>
            <li><a href="/about/company"><%= company %></a></li>
            <li><a href="/news?page=1"><%= news %></a></li>
            <li><a href="/about/identify"><%= identify %></a></li>
            <li><a href="/about/jobs"><%= jobs %></a></li>
            <li><a href="/about/contact-us"><%= contact_us %></a></li>
            <li><a href="/about/investor"><%= investor %></a></li>
          </ul>
        </div>
      </li>
      <li class="mobile-login">
        <div class="login-wrapper">
          <a href="/account/login">
            <%= login %>
            <i class="icon-arrow-right"></i>
          </a>
          <a href="/account/register">
            <%= register %>
            <i class="icon-arrow-right"></i>
          </a>
        </div>
      </li>
      <li class="mobile-lang">
        <a href="?lang=zh">中文</a>
        &nbsp;&nbsp;|&nbsp;&nbsp;
        <a href="?lang=en">English</a>
      </li>
    </ul>
    <div class="header-right">
      <% if (false) { %>
        <div class="logined-wrapper">
          <a href="/account/profile" class="vemail"></a>
          <a class="logout" href="/account/logout"><%= logout %></a>
        </div>
      <% } else { %>
        <div class="login-wrapper">
          <a href="/account/login"><%= login %></a>
          <a class="signup" href="/account/register"><%= register %></a>
        </div>
      <% } %>
      <div class="lang" style="display: inline-block"><a href="?lang=en">English</a></div>
    </div>
  </div>
</div>