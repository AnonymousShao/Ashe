<%- include('../../../../components/header.ejs') %>
  <div class="volcano-product">
    <article >
      <div id="page">
        <!-- banner -->
        <section class="content-banner">
          <div class="wraper">
             <div class="word">
               <div class="word-title"><%= banner_title %></div>
               <div class="mobile-title">UOS 4.0</div>
               <div class="word-brief"><%= banner_brief %></div>
               <div class="word-describe"><%= banner_describe %></div>
             </div>
             <a href="/docs" class="product-doc"><%= product_doc %></a>
           </div>
        </section>
        <section class="content-main">
          <div class="speciality">
            <p class="title"><%= speciality_title %></p>
            <div class="detail">
              <div class="item">
                <img src="/static/assets/page-views/product/product/icon1.png">
                <div class="character compute">
                  <p class="intro"><%= speciality_one_intro %></p>
                  <p class="brief"><%= speciality_one_brief %></p>
                </div>
              </div>
              <div class="item">
                <img src="/static/assets/page-views/product/product/icon2.png">
                <div class="character pass">
                  <p class="intro"><%= speciality_two_intro %></p>
                  <p class="brief"><%= speciality_two_brief %></p>
                </div>
              </div>
              <div class="item">
                <img src="/static/assets/page-views/product/product/icon3.png">
                <div class="character">
                  <p class="intro"><%= speciality_three_intro %></p>
                  <p class="brief"><%= speciality_three_brief %></p>
                  </div>
              </div>
              <div class="item">
                <img src="/static/assets/page-views/product/product/icon4.png">
                <div class="character pass">
                  <p class="intro"><%= speciality_four_intro %></p>
                  <p class="brief"><%= speciality_four_brief %></p>
                </div>
              </div>
              <div class="item">
                <img src="/static/assets/page-views/product/product/icon5.png">
                <div class="character compute">
                  <p class="intro"><%= speciality_five_intro %></p>
                  <p class="brief"><%= speciality_five_brief %></p>
                </div>
              </div>
              <div class="item">
                <img src="/static/assets/page-views/product/product/icon6.png">
                <div class="character">
                  <p class="intro"><%= speciality_six_intro %></p>
                  <p class="brief"><%= speciality_six_brief %></p>
                </div>
              </div>
            </div>
          </div>
          <div class="framework">
            <p class="title"><%= product_framework %></p>
            <img src="/static/assets/page-views/product/product/framework@v2.0.jpg">
          </div>
            <div class="product">
              <div class="inner">
                <div class="title"><%= product_title %></div>
                <div class="brief"><%= product_brief %></div>
              </div>
            </div>
            <div class="design">
              <div class="inner">
                <div class="item">
                  <div class="list list1">
                    <div class="left">
                      <img class="order" src="/static/assets/5-2-0.png">
                      <div class="title">UOS Director</div>
                      <div class="line"></div>
                      <div class="detail"><%= design_detail_one %></div>
                      <div class= "intro"><%= design_detail_one_two %></div>
                      <div class="btn">
                        <a class="see-doc" href="/docs/director"><%= see_doc %></a>
                        <a class="mobile-see-doc" href="/docs/director"><%= see_doc %>&nbsp;></a>
                        <a class="more-detail" href="/product/uos-director"><%= details %></a>
                      </div>
                    </div>
                    <div class="right">
                      <img src="/static/assets/5-1@v2.0.jpg">
                    </div>
                  </div>
                </div>
                <div class="item">
                  <div class="list list2">
                    <div class="left">
                      <img class="order" src="/static/assets/5-1-0.png">
                      <div class="title">UOS Enterprise</div>
                      <div class="line"></div>
                      <div class="detail"><%= design_detail_two %></div>
                      <div class= "intro"><%= design_detail_two_two %></div>
                      <div class="btn">
                        <a class="see-doc" href="/docs"><%= see_doc %></a>
                        <a class="mobile-see-doc" href="/docs"><%= see_doc %>&nbsp;></a>
                        <a class="more-detail" href="/product/uos-enterprise"><%= details %></a>
                      </div>
                    </div>
                    <div class="right">
                       <img src="/static/assets/5-2@v2.0.jpg">
                    </div>
                  </div>
                </div>
                <div class="item">
                  <div class="list list3">
                    <div class="left">
                      <img class="order" src="/static/assets/5-3-0.png">
                      <div class="title">UOS Manager</div>
                      <div class="line"></div>
                      <div class="detail"><%= design_detail_three %></div>
                      <div class= "intro"><%= design_detail_three_two %></div>
                      <div class="btn">
                        <a class="see-doc" href="/docs/halo"><%= see_doc %></a>
                        <a class="mobile-see-doc" href="/docs/halo"><%= see_doc %>&nbsp;></a>
                        <a class="more-detail" href="/product/uos-halo"><%= details %></a>
                      </div>
                    </div>
                    <div class="right">
                       <img src="/static/assets/5-3@v2.0.jpg">
                    </div>
                  </div>
                </div>
                <div class="item last">
                  <div class="list list4">
                    <div class="left">
                      <img class="order" src="/static/assets/5-4-0.png">
                      <div class="title">UOS Operator</div>
                      <div class="line"></div>
                      <div class="detail"><%= design_detail_four %></div>
                      <div class= "intro"><%= design_detail_four_two %></div>
                      <div class="btn">
                        <a class="see-doc" href="/docs/operator"><%= see_doc %></a>
                        <a class="mobile-see-doc" href="/docs/operator"><%= see_doc %>&nbsp;></a>
                        <a class="more-detail" href="/product/uos-operation-monitor"><%= details %></a>
                      </div>
                    </div>
                    <div class="right">
                       <img src="/static/assets/5-4@v2.0.jpg">
                    </div>
                  </div>
                </div>
              </div>
            </div>
        </section>
      </div>
    </article>
  </div>
<%- include('../../../../components/footer.ejs') %>